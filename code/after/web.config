<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>   
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>   
    <rewrite>
      <rules>
        <clear />
        <rule name="app">
           <match url="/*" negate="true" />                 
          <action type="Rewrite" url="server.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer> 
</configuration>